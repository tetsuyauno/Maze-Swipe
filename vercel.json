{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
            "source": "/web/(.*)",
            "destination": "/dist/$1"
        },
        {
            "source": "/web",
            "destination": "/dist/index.html"
        },
        {
            "source": "/(.*)",
            "destination": "server_dist/index.js"
        }
    ]
}