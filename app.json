{
  "expo": {
    "name": "Maze Game",
    "slug": "maze-game",
    "version": "1.0.0",
    "orientation": "landscape",
    "icon": "./assets/images/icon.png",
    "scheme": "mazegame",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.maze.game"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#1A1A2E",
        "foregroundImage": "./assets/images/icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.maze.game"
    },
    "web": {
      "output": "single",
      "favicon": "./assets/images/favicon.png",
      "themeColor": "#1A1A2E",
      "backgroundColor": "#1A1A2E",
      "display": "standalone",
      "orientation": "landscape",
      "name": "Maze Adventure",
      "shortName": "Maze Game",
      "description": "A fun and challenging maze swipe game.",
      "lang": "en",
      "startUrl": "/"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#1A1A2E",
          "dark": {
            "backgroundColor": "#1A1A2E"
          }
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "reactCompiler": true
    }
  }
}